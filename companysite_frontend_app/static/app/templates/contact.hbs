
<div class="contact">
	<form id='formID' {{action save on="submit"}}>
		<div>
	    {{
		    view Ember.TextField
		    valueBinding="email"
		    class="validate[custom[email]]"
		    placeholder="Enter Email Address"
		    required="true"
		}}
		</div>
		<div>
	    {{
	    	view Ember.TextArea
	    	valueBinding="txt"
	    	class="validate[required]"
	    	placeholder="Enter Text"
	    	required="true"
	    }}
		</div>
	     <button type="submit">Save</button>
	</form>
</div>
